<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="710dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="45dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/c" />
    <TextView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/time"
        android:layout_marginLeft="180dp"
        android:textStyle="bold"
        android:layout_marginTop="150dp"
        android:textSize="50dp"

        />

    <TextView
        android:id="@+id/count"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginTop="150dp"
        android:textSize="40dp"
        android:textAlignment="center"
        android:layout_weight="1"
        android:gravity="center_horizontal" />

    <ImageView
        android:id="@+id/imageViewnew1"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="285dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew2"
        android:layout_width="93dp"
        android:layout_height="122dp"
        android:layout_marginLeft="96dp"
        android:layout_marginTop="285dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew3"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="285dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />

    <ImageView
        android:id="@+id/imageViewnew4"
        android:layout_width="90dp"
        android:layout_marginLeft="5dp"
        android:layout_height="120dp"
        android:layout_marginTop="410dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew5"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="96dp"
        android:layout_marginTop="410dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew6"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="410dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew7"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="540dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew8"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="96dp"
        android:layout_marginTop="540dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />
    <ImageView
        android:id="@+id/imageViewnew9"
        android:layout_width="90dp"
        android:layout_height="120dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="540dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z" />

    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="#100E00"
        android:textColor="#ffffff"
        android:layout_marginTop="100dp"
        android:layout_height="wrap_content"
        android:text="START!" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="280dp"
        android:layout_marginTop="650dp"
        android:layout_height="40dp"
        android:background="#050000" />

</FrameLayout>